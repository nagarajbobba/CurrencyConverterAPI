{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "JwtSettings": {
    "Issuer": "yourIssuer",
    "Audience": "yourAudience",
    "SecretKey": "YourVeryLongSecretKeyThatIsAtLeast32CharactersLong",
    "ExpirationInMinutes": 60
  },
  "RedisCacheProvider": {
    "ConnectionString": "http://localhost:6379/ ,abortConnect=false",
    "ConnectionString_RD": "http://localhost:6379/ ,abortConnect=false",
    "ApplicationCode": "CC",
    "Environment": "DEV",
    "UseReadReplica": "false"

  },
  "CacheDuration": 60,
  "ExchangeSettings": {
    "ExcludedCurrencies": [
      "TRY",
      "PLN",
      "THB",
      "MXB"
    ],
    "FrankFurterApiProvider": "https://api.frankfurter.dev/v1/",
    "AnotherExchangeRatesApiProvider": "https://api.exchangeratesapi.io/",
    "ActiveProvider": "FrankFurterApiProvider"
  },
  "IpRateLimiting": {
    "EnableEndpointRateLimiting": true,
    "StackBlockedRequests": false,
    "RealIpHeader": "X-Real-IP",
    "ClientIdHeader": "X-ClientId",
    "HttpStatusCode": 429,
    "GeneralRules": [
      {
        "Endpoint": "*",
        "Period": "1m",
        "Limit": 10
      }
    ]
  },
  "IpRateLimitPolicies": {
    "IpRules": [
      {
        "Ip": "127.0.0.1",
        "Rules": [
          {
            "Endpoint": "*",
            "Period": "1m",
            "Limit": 20
          }
        ]
      }
    ]
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Seq" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341" // Replace with your Seq server URL
        }
      }
    ],
    "Enrich": [ "FromLogContext" ],
    "Properties": {
      "Application": "CurrencyConverter"
    }
  },
  "Jaeger": {
    "Host": "localhost",
    "Port": "6831"
  }

}
